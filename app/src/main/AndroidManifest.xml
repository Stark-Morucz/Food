<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.starkmorucz.food">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="me.starkmorucz.food.role.guest.DinnerActivity"></activity>
        <activity android:name="me.starkmorucz.food.role.guest.LunchActivity" />
        <activity android:name="me.starkmorucz.food.role.guest.BreakfastActivity" />
        <activity android:name="me.starkmorucz.food.role.user.MealItemActivity" />
        <activity android:name="me.starkmorucz.food.role.user.CookBookActivity" />
        <activity android:name="me.starkmorucz.food.DashboardActivity" />
        <activity android:name="me.starkmorucz.food.SignUpActivity" />
        <activity android:name="me.starkmorucz.food.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>